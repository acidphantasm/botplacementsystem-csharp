                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="How much time must pass before the spawn is triggered, in seconds. -1 is immediate." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Spawn Timer</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">@_kolontay_Time</MudButton>
                        </MudItem>
                        <MudItem xs="5">
                            <MudSlider @bind-Value="_kolontay_Time" ValueLabel="true" Min="-1" Max="3600" Color="Color.Info" Size="Size.Medium"/>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout._appUndoButtonToggle &&
                                 MainLayout.pendingChanges.Contains("_kolontay_Time"))
                            {
                                <MudButton @onclick="undo_Kolontay_Time" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle &&
                                     _kolontay_Time != -1)
                            {
                                <MudButton @onclick="default_Kolontay_Time" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>
                    </MudGrid>
                </MudExpansionPanel>
