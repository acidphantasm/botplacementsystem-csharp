                            @if (MainLayout._appUndoButtonToggle &&
                                 MainLayout.pendingChanges.Contains("_arenaFighterEvent_Time"))
                            {
                                <MudButton @onclick="undo_Smugglers_Time" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
