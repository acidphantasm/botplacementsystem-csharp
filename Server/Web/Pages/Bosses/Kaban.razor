                                 MainLayout.pendingChanges.Contains("_kaban_Time"))
                            {
                                <MudButton @onclick="undo_Kaban_Time" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle &&
                                     _kaban_Time != -1)
                            {
                                <MudButton @onclick="default_Kaban_Time" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>
                    </MudGrid>
