@page "/botplacementsystem/bossconfig/tagillahelperagro"
@layout MainLayout

<style>
    .mud-divider {
    border-width: 2.5px;
    }
</style>

<MudMainContent>
    <MudContainer Class="mb-20">
        <MudExpansionPanels MultiExpansion="true">
            <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Text="Enable/Disable" Expanded=true>
                <MudGrid>
                    <MudItem xs="1" />
                    <MudItem xs="4">
                        <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                            <MudTooltip Text="Allows TagillaHelperAgro to spawn if enabled, they will never spawn if disabled." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                <MudText>Enable TagillaHelperAgro</MudText>
                            </MudTooltip>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="6">
                        <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                            <MudSwitch T="bool" @bind-Value="_tagillaHelperAgro_Enabled" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_tagillaHelperAgro_Enabled</MudSwitch>
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="1">
                        @if (MainLayout._appUndoButtonToggle &&
                        MainLayout.pendingChanges.Contains("_tagillaHelperAgro_Enabled"))
                        {
                            <MudButton @onclick="undo_TagillaHelperAgro_Enable" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                        }
                        else if (MainLayout._appDefaultButtonToggle &&
                        _tagillaHelperAgro_Enabled != true)
                        {
                            <MudButton @onclick="default_TagillaHelperAgro_Enable" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                        }
                    </MudItem>
                </MudGrid>
            </MudExpansionPanel>
            @if (_tagillaHelperAgro_Enabled)
            {
                <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Text="General" Expanded=true>
                    <MudGrid>
                        
                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Adds additional spawns that you configure below per map." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Add Extra Spawns</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool?" @bind-Value="_TagillaHelperAgro_AddExtraSpawns" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_TagillaHelperAgro_AddExtraSpawns</MudSwitch>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            @if (MainLayout._appUndoButtonToggle &&
                           MainLayout.pendingChanges.Contains("_tagillaHelperAgro_AddExtraSpawns"))
                            {
                                <MudButton @onclick="undo_TagillaHelperAgro_AddExtraSpawns" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle &&
                            _TagillaHelperAgro_AddExtraSpawns != false)
                            {
                                <MudButton @onclick="default_TagillaHelperAgro_AddExtraSpawns" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>
                        <MudItem xs="1" />
                        <MudItem xs="4">
                            <MudPaper Class="d-flex align-center mud-width-full ma-1" Elevation="0">
                                <MudTooltip Text="Enabling this will remove the vanilla TagillaHelperAgro spawns from maps, if you still want tagillaHelperAgro - enable 'Add Extra Spawns' and set the spawns below." Color="Color.Primary" Placement="Placement.Right" Arrow="true">
                                    <MudText>Remove Vanilla Spawns</MudText>
                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="6">
                            <MudPaper Class="d-flex align-center mud-width-full" Elevation="0">
                                <MudSwitch T="bool?" @bind-Value="_TagillaHelperAgro_DisableVanillaSpawns" Color="Color.Success" UncheckedColor="Color.Default" LabelPlacement="Placement.End">@_TagillaHelperAgro_DisableVanillaSpawns</MudSwitch>
                            </MudPaper>
                        </MudItem>
