                                </MudTooltip>
                            </MudPaper>
                        </MudItem>
                        <MudItem xs="1">
                            <MudButton Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">@_tagillaHelperAgro_Time</MudButton>
                        </MudItem>
                        <MudItem xs="5">
                            <MudSlider @bind-Value="_tagillaHelperAgro_Time" ValueLabel="true" Min="-1" Max="3600" Color="Color.Info" Size="Size.Medium"/>
                        </MudItem>
                        <MudItem xs="1">
