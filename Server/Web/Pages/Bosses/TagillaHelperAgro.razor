                                 MainLayout.pendingChanges.Contains("_tagillaHelperAgro_Time"))
                            {
                                <MudButton @onclick="undo_TagillaHelperAgro_Time" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle &&
                                     _tagillaHelperAgro_Time != -1)
                            {
                                <MudButton @onclick="default_TagillaHelperAgro_Time" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
                        </MudItem>
                    </MudGrid>
                </MudExpansionPanel>
                <MudExpansionPanel Class="mud-expansion-panels-borders ma-3 pa-5" Style="padding-bottom:25px" Text="Spawn Chances & Zones" Expanded=true>
