                                 MainLayout.pendingChanges.Contains("_tagillaHelperAgro_DisableVanillaSpawns"))
                            {
                                <MudButton @onclick="undo_TagillaHelperAgro_DisableVanillaSpawns" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle &&
                                     _TagillaHelperAgro_DisableVanillaSpawns != false)
                            {
                                <MudButton @onclick="default_TagillaHelperAgro_DisableVanillaSpawns" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
