                            {
                                <MudButton @onclick="undo_TagillaAgro_DisableFollowers" Variant="Variant.Filled" Color="Color.Inherit" Size="Size.Small">Undo</MudButton>
                            }
                            else if (MainLayout._appDefaultButtonToggle &&
                                     _tagillaAgro_DisableFollowers != false)
                            {
                                <MudButton @onclick="default_TagillaAgro_DisableFollowers" Variant="Variant.Filled" Color="Color.Dark" Size="Size.Small">Default</MudButton>
                            }
